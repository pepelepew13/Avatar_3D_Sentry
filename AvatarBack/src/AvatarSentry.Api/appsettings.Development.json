{
  "JwtSettings": {
    "Issuer": "avatar-sentry",
    "Audience": "avatar-sentry-clients",
    "Key": "dev-key-change-me",
    "ExpiryHours": 8
  },
  "InternalApi": {
    "BaseUrl": "https://localhost",
    "AuthUser": "env:InternalApi__AuthUser",
    "AuthPassword": "env:InternalApi__AuthPassword"
  },
  "AzureStorage": {
    "ConnectionString": "env:AzureStorage__ConnectionString",
    "AccountName": "env:AzureStorage__AccountName",
    "ContainerNamePublic": "public",
    "ContainerNameTts": "tts",
    "BlobServiceEndpoint": "env:AzureStorage__BlobServiceEndpoint"
  },
  "AzureSpeech": {
    "SubscriptionKey": "env:AzureSpeech__SubscriptionKey",
    "Region": "env:AzureSpeech__Region",
    "DefaultLanguage": "es",
    "Voices": {
      "Es": "env:AzureSpeech__Voices__es",
      "En": "env:AzureSpeech__Voices__en",
      "Pt": "env:AzureSpeech__Voices__pt"
    }
  },
  "Dashboard": {
    "PanelUrl": "http://localhost:5168"
  }
}
